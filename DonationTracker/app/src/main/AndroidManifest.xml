<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kory.donationtracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Controller.StartUp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Controller.Login" />
        <activity android:name=".Controller.Home" />
        <activity android:name=".Controller.Registration" />
        <activity
            android:name=".Controller.LocEmpRegister"
            android:label="@string/title_activity_loc_emp_register"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>